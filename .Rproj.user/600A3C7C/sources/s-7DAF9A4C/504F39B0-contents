neuralNetwork <- setRefClass("neuralNetwork", fields = list(id = "numeric",
                                                            architecture = "character",
                                                            evaluated = "character",
                                                            loss = "numeric",
                                                            metric = "numeric",
                                                            model = "character"))

Main <- function(population.size) {
  population.individuals <- lapply(population.size, )
}

# BASE: http://r-pkgs.had.co.nz/package.html

individual <- neuralNetwork(id = 1, architecture = "nnn/nn/nnnn", evaluated = "FALSE", loss = 0.0327, metric = 0.9673, model = "model.h5")
individual
